   <translation id="cron_step2_help_m" qtlid="199663">Minuto (da 0 a 59)</translation>
   <translation id="cron_step2_help_mon" qtlid="199702">Mese (da 1 a 12, o da {0} a {1})</translation>
   <translation id="cron_step2_help_notation_1" qtlid="199741">tutte le unità</translation>
   <translation id="cron_step2_help_notation_2" qtlid="199754">solo il 2 e il 4</translation>
